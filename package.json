{
  "name": "create-express-api",
  "version": "1.0.0",
  "description": "An Express REST API boilerplate",
  "main": "index.js",
  "author": "Adnan Iqbal Khan",
  "license": "Apache-2.0",
  "scripts": {
    "start": "nodemon index.js localhost 8080",
    "dev": "node index.js",
    "test": "jest",
    "swagger": "node swagger.js"
  },
  "keywords": [
    "rest api",
    "mongodb",
    "express",
    "nodejs"
  ],
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AdnanIqbalKhan/create-express-api.git"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.5.2",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "jest": "^29.0.1",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "swagger-autogen": "^2.21.5"
  }
}